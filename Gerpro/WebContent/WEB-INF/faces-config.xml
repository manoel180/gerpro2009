<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
	<managed-bean>
		<managed-bean-name>propostaBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.PropostaBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>equipeBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.EquipeBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>artefatosBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.ArtefatosBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>usuarioBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.UsuarioBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>alocarBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.AlocarBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>construirPropostaBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.ConstruirPropostaBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>submeterPropostaBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.SubmeterPropostaBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>corrigirPropostaBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.CorrecaoBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>erroBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.ErroBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Relatorio -->
	<managed-bean>
		<managed-bean-name>relatorioBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.RelatorioBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<!-- Navegação para telas HOME -->
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>homeAluno</from-outcome>
			<to-view-id>/Aluno/homeAluno.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>homeProfessor</from-outcome>
			<to-view-id>/Professor/homeProfessor.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>homeCoordenador</from-outcome>
			<to-view-id>/Coordenador/homeCoordenador.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>erro</from-outcome>
			<to-view-id>/common/ErrorModalPage.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logoff</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
		<!-- Tela Pesquisar Proposta -->
		<navigation-case>
			<from-outcome>pesquisarProposta</from-outcome>
			<to-view-id>/Coordenador/PesquisarProposta.jsp</to-view-id>
		</navigation-case>
		<!-- Tela Pesquisar Artefatos -->
		<navigation-case>
			<from-outcome>go_manterArtefatos</from-outcome>
			<to-view-id>/Coordenador/PesquisarArtefatos.jsp</to-view-id>
		</navigation-case>
		<!-- Tela Pesquisar Equipe -->
		<navigation-case>
			<from-outcome>go_manterEquipe</from-outcome>
			<to-view-id>/Coordenador/PesquisarEquipe.jsp</to-view-id>
		</navigation-case>
		<!-- Tela Relatório Equipe -->
		<navigation-case>
			<from-outcome>go_relatorio</from-outcome>
			<to-view-id>/Relatorios/Relatorio.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Navegação da Área do Professor -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>corrigirCronograma</from-outcome>
			<to-view-id>/Professor/CorrigirCronograma.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirJustificativa</from-outcome>
			<to-view-id>
				/Professor/CorrigirJustificativa.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirListaFuncoes</from-outcome>
			<to-view-id>/Professor/CorrigirListaFuncoes.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirMetodologia</from-outcome>
			<to-view-id>/Professor/CorrigirMetodologia.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirMissao</from-outcome>
			<to-view-id>/Professor/CorrigirMissao.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listarPropostas</from-outcome>
			<to-view-id>/Professor/ListarPropostas.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirProposta</from-outcome>
			<to-view-id>/Professor/CorrigirProposta.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirAvaliacaoGeral1</from-outcome>
			<to-view-id>/Professor/CorrigirAvaliacaoGeralPergunta1.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirAvaliacaoGeral2</from-outcome>
			<to-view-id>/Professor/CorrigirAvaliacaoGeralPergunta2.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>corrigirAvaliacaoGeral7</from-outcome>
			<to-view-id>/Professor/CorrigirAvaliacaoGeralPergunta7.jsp</to-view-id>
		</navigation-case>		
		<navigation-case>
			<from-outcome>submeterCorrecao</from-outcome>
			<to-view-id>/Professor/SubmeterCorrecao.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Navegação da Área do Coordenador -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>go_altproposta</from-outcome>
			<to-view-id>/Coordenador/AltProposta.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
		<navigation-rule>
		<navigation-case>
			<from-outcome>alocarProposta</from-outcome>
			<to-view-id>/Coordenador/AlocarProposta.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Coordenador/PesquisarArtefatos.jsp</from-view-id>
		<navigation-case>
			<from-outcome>incluirArtefatos</from-outcome>
			<to-view-id>/Coordenador/CadastroArtefatos.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterarArtefatos</from-outcome>
			<to-view-id>/Coordenador/AlterarArtefatos.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>visualizarArtefatos</from-outcome>
			<to-view-id>/Coordenador/PesquisarArtefatos.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Coordenador/PesquisarProposta.jsp</from-view-id>
		<navigation-case>
			<from-outcome>incluirProposta</from-outcome>
			<to-view-id>/Coordenador/CadastroProposta.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterarProposta</from-outcome>
			<to-view-id>/Coordenador/AlterarProposta.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>visualizarProposta</from-outcome>
			<to-view-id>/Coordenador/PesquisarProposta.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Coordenador/PesquisarUsuario.jsp</from-view-id>
		<navigation-case>
			<from-outcome>incluirUsuario</from-outcome>
			<to-view-id>/Coordenador/CadastroUsuario.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterarUsuario</from-outcome>
			<to-view-id>/Coordenador/AlterarUsuario.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<!-- Navegação da Área do Aluno -->
	<navigation-rule>
		<navigation-case>
			<from-outcome>construirCronograma</from-outcome>
			<to-view-id>/Aluno/ConstruirCronograma.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>construirJustificativa</from-outcome>
			<to-view-id>/Aluno/ConstruirJustificativa.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>construirListaFuncoes</from-outcome>
			<to-view-id>/Aluno/ConstruirListaFuncoes.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>construirMetodologia</from-outcome>
			<to-view-id>/Aluno/ConstruirMetodologia.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>construirMissao</from-outcome>
			<to-view-id>/Aluno/ConstruirMissao.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>go_ConstruirProposta</from-outcome>
			<to-view-id>/Aluno/ConstruirProposta.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>SubmeterProposta</from-outcome>
			<to-view-id>/Aluno/SubmeterProposta.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Tratamento das Mensagens de Exceção -->
	<application>
		<message-bundle>
			br.com.gerpro.mensagens.mensagens
		</message-bundle>
	</application>
	<lifecycle>
		<phase-listener>
			br.com.gerpro.mensagens.MessageListener3
		</phase-listener>
	</lifecycle>
</faces-config>
