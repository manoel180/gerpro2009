	</managed-bean>
	<managed-bean>
		<managed-bean-name>submeterPropostaBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.action.SubmeterPropostaBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<!-- Relatorio -->
	<managed-bean>
		<managed-bean-name>relatorioBean</managed-bean-name>
		<managed-bean-class>
			br.com.gerpro.relatorios.ActionRelatorio
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>homeAluno</from-outcome>
			<to-view-id>/Aluno/homeAluno.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>homeProfessor</from-outcome>
			<to-view-id>/Professor/homeProfessor.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>homeCoordenador</from-outcome>
			<to-view-id>/Coordenador/homeCoordenador.jsp</to-view-id>
		</navigation-case>
			
		<navigation-case>
			<from-outcome>go_manterArtefatos</from-outcome>
			<to-view-id>/ManterArtefatos/PesquisarArtefatos.jsp</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>go_manterEquipe</from-outcome>
			<to-view-id>/ManterEquipe/PesquisarEquipe.jsp</to-view-id>
		</navigation-case>
	
		
		<navigation-case>
			<from-outcome>go_manterProposta</from-outcome>
			<to-view-id>
				/ManterProposta/PesquisarProposta.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>go_SubmeterProposta</from-outcome>
			<to-view-id>
				/SubmeterProposta/SubmeterProposta.jsp
			</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>go_ConstruirProposta</from-outcome>
			<to-view-id>
				/ConstruirProposta/ConstruirProposta.jsp
			</to-view-id>
		</navigation-case>
		
		<!-- Teste  Nao mexa  Marcelo -->
			<navigation-case>
			<from-outcome>listarPropostas</from-outcome>
			<to-view-id>
				/Professor/listarProposta.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- Navegação do Aluno -->
	<navigation-rule>
