<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!-- Configuração dos beans -->
	<managed-bean>
		<managed-bean-name>propostaBean</managed-bean-name>
		<managed-bean-class>Action.PropostaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>equipeBean</managed-bean-name>
		<managed-bean-class>Action.EquipeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>go_manterEquipe</from-outcome>
			<to-view-id>/ManterEquipe/PesquisarEquipe.jsp</to-view-id>
		</navigation-case>	
	</navigation-rule>
	
	<!-- Navegaçao das pagina Proposta -->
	<navigation-rule>
		<from-view-id>
			/ManterProposta/PesquisarProposta.jsp
		</from-view-id>
		<navigation-case>
			<from-outcome>incluir</from-outcome>
			<to-view-id>
				/ManterProposta/CadastroProposta.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterar</from-outcome>
			<to-view-id>/ManterProposta/AlterarProposta.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>visualizar</from-outcome>
			<to-view-id>
				/ManterProposta/PesquisarProposta.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<!-- Navegaçao das pagina Equipe -->
	<navigation-rule>
		<from-view-id>/ManterEquipe/PesquisarEquipe.jsp</from-view-id>
		<navigation-case>
			<from-outcome>incluir</from-outcome>
			<to-view-id>/ManterEquipe/CadastroEquipe.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alterar</from-outcome>
			<to-view-id>/ManterEquipe/AlterarEquipe.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>visualizar</from-outcome>
			<to-view-id>/ManterEquipe/PesquisarEquipe.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
